  . $ZDOTFILESDIR/local.zsh
fi

export PATH=$PATH:$ZDOTFILESDIR/bin
export PATH=$PATH:$HOME/bin
export PATH=$PATH:$HOME/.local/bin
export PATH=$PATH:$HOME/code/naoshima/codec
export PATH=$PATH:$HOME/dotfiles/bin/macos

